<div class="container">
  <table>
    <thead>
      <tr>
        <th>Title</th>
        <th>Date</th>
        <th>Edit</th>
        <th>Delete</th>
      </tr>
    </thead>
    <tbody>
      <% @posts.each do |post| %>
        <tr>
          <td> <%= post.title %> </td>
          <td> <%= post.created_at %> </td>
          <td> <%= link_to 'Edit', edit_post_path(post) %> </td>
          <td> <%= link_to 'Delete', post_path(post), method: :delete %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
